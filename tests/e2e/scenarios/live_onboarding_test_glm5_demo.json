    "min_protocol_steps": 2,
    "terminal_protocol_status": "completed",
    "artifact_patterns": [
      ".protocols/*/plan.md",
      ".devgodzilla/steps/*/artifacts/execution.json"
    ]
  },
  "timeouts": {
    "onboard_seconds": 2700,
    "planning_seconds": 2700,
    "execution_seconds": 2700
  },
  "retries": {
    "max_attempts": 3,
    "backoff_seconds": 2,
    "max_backoff_seconds": 20
  }
}
