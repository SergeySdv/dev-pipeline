{
  "scenario_id": "live_onboarding_test_glm5_demo",
  "source": "github_user_allowlist",
  "repo": {
    "owner": "ilyafedotov-ops",
    "name": "test-glm5-demo",
    "url": "https://github.com/ilyafedotov-ops/test-glm5-demo.git",
    "default_branch": "main"
  },
  "adapter_id": "test_glm5_demo",
  "workflow": {
    "stages": [
      "project_create",
      "project_onboard_windmill",
      "protocol_feature_cycles"
    ]
  },
  "expectations": {
    "discovery_outputs": [
      "specs/discovery/_runtime/DISCOVERY.md",
      "specs/discovery/_runtime/DISCOVERY_SUMMARY.json"
