{
  "summary": "Execute Protocol Flow",
  "description": "Execute a single step_run_id by calling the DevGodzilla API (QA auto-run in DevGodzilla).",
  "value": {
    "modules": [
      {
        "id": "execute_step",
        "value": {
          "type": "script",
          "path": "u/devgodzilla/step_execute_api"
        },
        "input_transforms": {
          "step_run_id": { "type": "javascript", "expr": "flow_input.step_run_id" }
        }
      }
    ]
  },
  "schema": {
        "type": "object",
        "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
      "step_run_id": { "type": "integer", "description": "Step run ID", "default": 1 }
    },
    "required": [
      "step_run_id"
    ]
  }
}
